{
  "presets": ["@babel/preset-env", "@babel/preset-react", "@babel/preset-typescript", "@linaria"],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ],
    [
      "module-resolver",
      { "root": ["./src/"], "extensions": [".tsx", ".ts", ".js", ".json"], "loglevel": "warn" }
    ]
  ],
  "targets" : "defaults, not IE 11"
}
